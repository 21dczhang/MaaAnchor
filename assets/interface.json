{
    "interface_version": 2,
    "name": "MaaMaoDianJiangLin",
    "description": "锚点降临自动化助手",
    "license": "MIT",
    "github": "https://github.com/21dczhang/MaaMaoDianJiangLin",
    "version": "DEBUG",
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb",
            "display_short_side": 720
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "./resource"
            ]
        }
    ],
    "task": [
        {
            "name": "启动游戏",
            "entry": "StartGame_Start",
            "default_check": true
        },
        {
            "name": "基建领取  & 派遣",
            "entry": "基建_Start",
            "option": [
                "基建机器人加速选择"
            ],
            "default_check": true
        },
        {
            "name": "领取商城免费礼包",
            "entry": "商城免费礼包_Start",
            "default_check": true
        },
        {
            "name": "联盟筹备",
            "entry": "联盟_Start",
            "default_check": true
        },
        {
            "name": "演习",
            "entry": "演习_Start",
            "default_check": true
        },
        {
            "name": "情报副本",
            "entry": "情报副本_Start",
            "description": "先偷懒，固定在情报副本第一页查找，后续有新增的需要先在情报副本添加特别关注。并且需要先手动通过一次EP3",
            "default_check": true,
            "option": [
                "情报角色选择"
            ]
        },
        {
            "name": "领取体力",
            "entry": "领取体力_Start",
            "default_check": true
        },
        {
            "name": "领取邮箱",
            "entry": "领取邮箱_Start",
            "default_check": true
        },
        {
            "name": "神血免费抽",
            "entry": "神血铸痕免费抽_Start",
            "default_check": true
        },
        {
            "name": "特供免费礼包",
            "entry": "特供免费礼包_Start",
            "default_check": true
        },
        {
            "name": "战斗-消耗体力",
            "entry": "战斗_Start",
            "description": "目前只支持自动打模组",
            "option": [
                "战斗-模组选择"
            ],
            "default_check": true
        },
        {
            "name": "事务奖励领取",
            "entry": "日常奖励领取_Start",
            "default_check": true
        },
        {
            "name": "无限城",
            "entry": "无限城_Start",
            "description": "需默认配置能打得过的队伍",
            "default_check": true
        },
        {
            "name": "测试任务2",
            "entry": "战斗_战斗_Goto_模组测试_选择模组2_选择次数_Start"
        }
    ],
    "option": {
        "情报角色选择": {
            "type": "input",
            "label": "情报角色1",
            "description": "选择情报角色1，需先在情报中特别关注，确保角色在第一页，后续会改进...搁置中",
            "inputs": [
                {
                    "name": "角色1名称",
                    "label": "角色1名称",
                    "description": "角色1名称,可以写角色名字的某个字段即可，不用全名也行",
                    "default": "白蔷薇",
                    "pipeline_type": "string"
                },
                {
                    "name": "角色2名称",
                    "label": "角色2名称",
                    "description": "角色2名称,可以写角色名字的某个字段即可，不用全名也行",
                    "default": "鸣晔",
                    "pipeline_type": "string"
                }
            ],
            "pipeline_override": {
                "情报副本_情报副本_角色1": {
                    "expected": "{角色1名称}"
                },
                "情报副本_情报副本_角色2": {
                    "expected": "{角色2名称}"
                }
            }
        },
        "基建机器人加速选择": {
            "type": "select",
            "description": "基建机器人加速选项，目前只支持提纯车间和总生产线，并且总生产线需手动设置好模板",
            "default_case": "提纯车间",
            "cases": [
                {
                    "name": "提纯车间",
                    "pipeline_override": {
                        "基建_基建_Start_机器人加速": {
                            "expected": "提纯"
                        }
                    }
                },
                {
                    "name": "总生产线",
                    "pipeline_override": {
                        "基建_基建_Start_机器人加速": {
                            "expected": "总生"
                        }
                    }
                }
            ]
        },
        "战斗-模组选择": {
            "type": "select",
            "default_case": "森罗侵浸",
            "cases": [
                {
                    "name": "勇士之力",
                    "pipeline_override": {
                        "战斗_战斗_Goto_模组测试_选择模组2": {
                            "expected": [
                                "勇士之力"
                            ]
                        }
                    }
                },
                {
                    "name": "千雷麟集",
                    "pipeline_override": {
                        "战斗_战斗_Goto_模组测试_选择模组2": {
                            "expected": [
                                "千雷"
                            ]
                        }
                    }
                },
                {
                    "name": "炽天炽地",
                    "pipeline_override": {
                        "战斗_战斗_Goto_模组测试_选择模组2": {
                            "expected": [
                                "炽地"
                            ]
                        }
                    }
                },
                {
                    "name": "霜寒蚀体",
                    "pipeline_override": {
                        "战斗_战斗_Goto_模组测试_选择模组2": {
                            "expected": [
                                "霜寒"
                            ]
                        }
                    }
                },
                {
                    "name": "森罗侵浸",
                    "pipeline_override": {
                        "战斗_战斗_Goto_模组测试_选择模组2": {
                            "expected": [
                                "森罗"
                            ]
                        }
                    }
                },
                {
                    "name": "辉耀之鸣",
                    "pipeline_override": {
                        "战斗_战斗_Goto_模组测试_选择模组2": {
                            "expected": [
                                "辉耀之鸣"
                            ]
                        }
                    }
                },
                {
                    "name": "怒涛之势",
                    "pipeline_override": {
                        "战斗_战斗_Goto_模组测试_选择模组2": {
                            "expected": [
                                "怒涛之势"
                            ]
                        }
                    }
                },
                {
                    "name": "心之鸣动",
                    "pipeline_override": {
                        "战斗_战斗_Goto_模组测试_选择模组2": {
                            "expected": [
                                "心之鸣动"
                            ]
                        }
                    }
                },
                {
                    "name": "攻击增幅",
                    "pipeline_override": {
                        "战斗_战斗_Goto_模组测试_选择模组2": {
                            "expected": [
                                "攻击增幅"
                            ]
                        }
                    }
                },
                {
                    "name": "生命增幅",
                    "pipeline_override": {
                        "战斗_战斗_Goto_模组测试_选择模组2": {
                            "expected": [
                                "生命增幅"
                            ]
                        }
                    }
                },
                {
                    "name": "灵犀明鹿",
                    "pipeline_override": {
                        "战斗_战斗_Goto_模组测试_选择模组2": {
                            "expected": [
                                "明鹿"
                            ]
                        }
                    }
                },
                {
                    "name": "雳闪穹鸣",
                    "pipeline_override": {
                        "战斗_战斗_Goto_模组测试_选择模组2": {
                            "expected": [
                                "穹鸣"
                            ]
                        }
                    }
                }
            ]
        }
    }
}